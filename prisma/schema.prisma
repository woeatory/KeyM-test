generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_CONNECTION_STRING")
}

model Booking {
  id        String @id @map("_id")
  user      String
  date      String
  startTime String
  endTime   String
}
